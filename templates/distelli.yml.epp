DistelliAccessToken: '<%= $pipelines::agent::access_token.unwrap %>'
DistelliSecretKey: '<%= $pipelines::agent::secret_key.unwrap %>'
<%  if $pipelines::agent::endpoint { -%>
AgentEndpoint: '<%= $pipelines::agent::endpoint %>'
<%  }
    if $pipelines::agent::environments and $pipelines::agent::environments.size > 0 { -%>
Environments:
<%      $pipelines::agent::environments.each |$env| { -%>
  - <%= $env %>
<%      }
    } %>
